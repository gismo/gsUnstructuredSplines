      <Basis type="BSplineBasis" index="0">
	<KnotVector degree="3">0 0 0 0 1 1 1 1 </KnotVector>
      </Basis>
      <Basis type="BSplineBasis" index="1">
	<KnotVector degree="3">0 0 0 0 1 1 1 1 </KnotVector>
      </Basis>
    </Basis>
    <coefs geoDim="3">
     -0.5 -0.5 0.0 
     -0.375 -0.5 0.5
     -0.185 -0.5 0.25
      0.0 -0.5 0.0
     -0.5 -0.375 0.0 
