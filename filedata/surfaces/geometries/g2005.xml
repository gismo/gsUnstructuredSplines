     -0.185 -0.185 0.2815
      0.0 -0.185 0.0
     -0.5 0.0 0.0
     -0.375 0.0 0.5
     -0.185 0.0 0.25
      0.0 0.0 0.0
    </coefs>
  </Geometry>

